        <key>value</key>
        <value>
          <operator>
            <type>AND</type>
            <operator>
              <type>@image</type>
            </operator>
            <operator>
              <type>POSITION</type>
              <attribute>
                <key>element</key>
                <value>:image</value>
                <key>sequence</key>
